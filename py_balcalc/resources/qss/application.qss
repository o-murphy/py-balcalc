/* QCheckBox */

QGroupBox {
	color: rgb(255, 255, 255);
	border: 1px solid rgb(78, 78, 78);
	padding-top: 25px;
    font-size: 14px;
}

QGroupBox::title {
	padding: 5px 20px 5px 20px;
	color: "orange";
	border-top-left-radius: 10px;
	border-bottom-right-radius: 10px;
	background-color: rgb(40, 40, 40);
    font-size: 14px;
}

QGroupBox#ProfileWeapon::indicator {
    image: url(":/munition/rifle.svg");
}

QGroupBox#ProfileCartridge::indicator {
    image: url(":/munition/cartridge.svg");
}

QGroupBox#ProfileBullet::indicator {
    image: url(":/munition/bullet.svg");
}

QGroupBox#ProfileConditions::indicator {
    image: url(":/icons/water-thermometer-outline-orange.svg");
}

QGroupBox#ProfileA7PMeta::indicator {
    image: url(":/icons/card-bulleted-outline-orange.svg");
}

QCheckBox {
	padding-left: 5px;
	padding-right: 5px;
	font-size: 14px;
	color: white;
}

QCheckBox::indicator:unchecked {
	background-color: rgb(255, 255, 255);
}

QCheckBox::indicator:unchecked:hover {
	background-color:  rgb(255, 255, 255);
	image: url(":/icons/check-black.svg");
}

QCheckBox:disabled {
	color: rgb(78, 78, 78);
}

QCheckBox::indicator:checked {
    background: "orange";
    image: url(":/icons/check-black.svg");
}
QCheckBox::indicator:unchecked {
    background: white;
}

/* QComboBox */


QComboBox:enabled {
    background-color: rgb(40, 40, 40);
    color: white;
    border: 0px;
    font-size: 14px;
}

QComboBox:disabled {
    background-color: rgb(40, 40, 40);
    color: white;
    border: 0px;
    font-size: 14px;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    background: transparent;
	image: url(":/icons/menu-down-white.svg");
    width: 20px;
}

QComboBox::drop-down:hover {
	background-color: rgb(75, 75, 75);
	border: 0px;
	border-radius: 2px;
}

QComboBox::drop-down:disabled {
    image: none;
}




/* QRadioButton */

QRadioButton{
	padding-left: 5px;
	padding-right: 5px;
	font-size: 14px;
	color: white;
}
QRadioButton::indicator {
     width: 15px;
     height: 15px;
     border: 0px;
     border-radius: 7px;
}

QRadioButton::indicator {
		border: 0px;
		border-radius: 7px;
}

QRadioButton::indicator:checked {
		background-color: "orange";
		border: 0px;
		border-radius: 4px;
		image: url(":/icons/check-black");
}

QRadioButton::indicator:unchecked {
	background-color: rgb(255, 255, 255);
}

QRadioButton::indicator:disabled, QRadioButton::indicator:unchecked:disabled {
	background-color:rgb(98, 98, 98);
}


/* QToolTip { color: #ffffff; background-color: #2a82da; border: 1px solid white; } */
QToolTip {
    color: #ffffff;
    background-color: rgb(40, 40, 40);
    border: 0px solid rgb(75, 75, 75);
    padding: 2px 4px 2px 4px;
}

/* QSpinBox, QDoubleSpinBox */


QSpinBox:enabled, QDoubleSpinBox:enabled {
	background-color: rgb(40, 40, 40);
	color: rgb(255, 255, 255);
}

QSpinBox:disabled, QDoubleSpinBox:disabled {
	background-color: rgb(40, 40, 40);
}

QSpinBox::up-button,
QDoubleSpinBox::up-button {
	width: 20px;
	image: url(":/icons/menu-up-white.svg");
}
QSpinBox::down-button,
QDoubleSpinBox::down-button {
	width: 20px;
	image: url(":/icons/menu-down-white.svg");
}
QSpinBox::down-button:hover,
QSpinBox::up-button:hover,
QDoubleSpinBox::down-button:hover,
QDoubleSpinBox::up-button:hover {
		background-color: rgb(75, 75, 75);
		border: 0px;
		border-radius: 2px;
}

QSpinBox::down-button:disabled,
QSpinBox::up-button:disabled,
QDoubleSpinBox::down-button:disabled,
QDoubleSpinBox::up-button:disabled {
    image: null;
}
/* else */

QWidget {
	background-color: rgb(51, 51, 51);
	color: rgb(255, 255, 255);
    font-size: 14px;
}

QWidget#MainWindow {
    background-color:rgb(28, 28, 28);
}

QLineEdit {
	border: 0px solid rgb(128,128,128);
	background-color: rgb(40, 40, 40);
	font-size: 14px;
}

QLabel {
	font-size: 14px;
}

QTableWidget, QTableView {
    outline: 0;
	background-color: rgb(40, 40, 40);
}

QHeaderView::section {
	background-color: rgb(51, 51, 51);
}

QTableWidget::item:selected, QTableView::item:selected {
	background-color: rgb(51, 51, 51);
}

QTableCornerButton::section {
    background: rgb(51, 51, 51);
}

QPushButton, QCommandLinkButton {
    border: 1px solid rgb(76, 76, 76);
	font-size: 14px;
	border-radius: 4px;
    padding: 2px 10px 2px 10px;
}

QToolButton {
    border: 0;
	font-size: 14px;
}

QPushButton:hover, QToolButton:hover, QCommandLinkButton:hover {
	border: 1px solid rgb(76, 76, 76);
	border-radius: 4px;
	background-color: rgb(76, 76, 76);
}

QTabBar::tab {
	height: 25px; width: 200px;
	font-size: 15px;
    padding-left: 15px;
    padding-right: 15px;
}


QWidget:disabled {
	color: rgb(160, 160, 160);
	font-size: 14px;
}

QTabBar::tab:top {
	height: 25px;
	background-color:rgb(28, 28, 28);
	color: rgb(95, 95, 95);
	border-bottom: 2px solid rgb(28, 28, 28);
}

QTabBar::close-button {
 image: url(":/icons/window-close-white.svg");
 subcontrol-position: right;
}

QTabBar::close-button:hover {
 image: url(":/icons/window-close-orange.svg");
 subcontrol-position: right;
}


#MainTabWidget QTabBar::tab:top {
	height: 35px;
    padding: 2px 10px 2px 10px;
}

#profileCurrent QTabBar::tab:top {
	height: 25px; width: 200px;
}

QTabBar::tab:top:selected {
	color: "orange";

    border-bottom: 2px solid "orange";
}

QTabBar::tab:top:hover:!selected {
    color: rgb(255, 255, 255);
}

QTabBar::tab:left {
	background-color:rgb(28, 28, 28);
	color: rgb(95, 95, 95);
	border-right: 2px solid rgb(28, 28, 28);

    height: 100px;
    width: 30px;
}

QTabBar::tab:left:selected {
	color: "orange";
    border-right: 2px solid "orange";
}

QTabBar::tab:left:hover:!selected {
    color: rgb(255, 255, 255);
}

QTabWidget {
    background-color: rgb(28, 28, 28);
    background: rgb(28, 28, 28);
}

QTabWidget::tab-bar {
    background-color: rgb(28, 28, 28);
}

QTabWidget::pane {
	border: 2px solid rgb(76, 76, 76);
}

QWidget QPushButton#add {
	background: rgb(51, 51, 51);
	color: rgb(95, 95, 95);
	font-size: 14px;
	border: 0px;
}

QWidget QPushButton#add:hover {
	background-color: rgb(76, 76, 76);
	border-radius: 0px;
}

