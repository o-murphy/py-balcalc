QCalendarWidget QToolButton {
  	height: 40px;
  	width: 100px;
  	color: white;
  	font-size: 18px;
  	icon-size: 16px 16px;
  	background-color: transparent;
}

QCalendarWidget QMenu {
    width: 100px;
    left: 20px;
    color: white;
    font-size: 12px;
    background-color: rgb(51, 51, 51);
}

QCalendarWidget QToolButton::menu-indicator {
    image: none;
}

#qt_calendar_prevmonth, #qt_calendar_nextmonth {
    qproperty-icon: none;
    background-color: transparent;
}

#qt_calendar_prevmonth {
    qproperty-text: "<";
}

#qt_calendar_nextmonth {
    qproperty-text: ">";
}

#qt_calendar_calendarview {
    selection-background-color: rgb(64, 64, 64);
}

QCalendarWidget QTableView{

    alternate-background-color: rgb(51, 51, 51);
}

QDateEdit {
    border-style: solid;
    border-width: 0px;
    border-color: rgb(64, 64, 64);
    spacing: 5px;
    background-color: rgb(40, 40, 40);
	font-size: 14px;
}

QDateEdit::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    background: transparent;
	image: url(":/icons/menu-down-white.svg");
    width: 20px;
}

QDateEdit::drop-down:hover {
	background-color: rgb(75, 75, 75);
	border: 0px;
	border-radius: 2px;
}

QDateEdit::drop-down:disabled {
    image: none;
}

QTreeView {
	border: 1px solid rgb(78, 78, 78);
    alternate-background-color: rgb(75, 75, 75);
}

QTreeView QHeaderView {
    border: 0px solid rgb(78, 78, 78);
    background-color: rgb(40, 40, 40);
}

QTreeView::section QAbstractItemView::section {
    border: 1px solid rgb(78, 78, 78);
    background-color: transparent;
    padding: 2px 10px 2px 10px;
}

QTreeView QScrollBar:vertical {
    background-color: rgb(51, 51, 51); /*#2A2929;*/
    width: 14px;
    margin: 15px 6px 15px 6px;
    border: 1px transparent #2A2929;
    border-radius: 6px;
}

QTreeView QScrollBar:vertical:hover {
    background-color: rgb(51, 51, 51);
    width: 14px;
    margin: 15px 3px 15px 3px;
    border: 1px transparent #2A2929;
    border-radius: 6px;
}

QTreeView QScrollBar::handle:vertical {
    background-color: rgb(78, 78, 78);
    min-height: 5px;
    border-radius: 4px;
}

QTreeView QScrollBar::sub-line:vertical
{
    margin: 4px 0px 0px 0px;
    border: 1px solid rgb(51, 51, 51);
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    background-color: rgb(51, 51, 51);
    color: white;
    height: 5px;
    width: 8px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QTreeView QScrollBar::add-line:vertical
{
    margin: 0px 0px 4px 0px;
    border: 1px solid rgb(51, 51, 51);
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;
    background-color: rgb(51, 51, 51);
    color: white;
    height: 5px;
    width: 8px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QTreeView QScrollBar::sub-line:vertical:hover,QScrollBar::sub-line:vertical:on {
    margin: 4px 0px 0px 0px;
    border: 1px solid rgb(51, 51, 51);
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    background-color: rgb(78, 78, 78);
    height: 5px;
    width: 8px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QTreeView QScrollBar::add-line:vertical:hover, QScrollBar::add-line:vertical:on {
    margin: 0px 0px 4px 0px;
    border-bottom: 1px solid rgb(78, 78, 78);
    background-color: rgb(78, 78, 78);
    height: 5px;
    width: 8px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QTreeView QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
    background: none;
}

QTreeView QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

QTableView::section QAbstractItemView::section {
    border-top: 1px solid rgb(40, 40, 40);
    /* border-top: 1px solid rgb(78, 78, 78); */
    background-color: rgb(28, 28, 28);
    padding: 2px 4px 2px 4px;
}

QTableView::section:selected QAbstractItemView::section:selected {
    border: 1px solid rgb(28, 28, 28);
    background-color: rgb(28, 28, 28);
    padding: 2px 4px 2px 4px;
}

QTableWidget::item, QTableView::item {
    padding-left: 4px;
    padding-right: 4px;
}



QTableWidget::item:selected, QTableView::item:selected {
	background-color: rgb(51, 51, 51);
}

QTableCornerButton::section {
    background: rgb(28, 28, 28);
}

